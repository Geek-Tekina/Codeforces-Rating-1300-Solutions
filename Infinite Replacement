#include<bits/stdc++.h>
using namespace std;

int main()
{  int t ;
cin>>t;
while(t--)
   { string s,t;
    cin>>s>>t;
    
    bool flag =0;
    for(auto it : t)
    {
        if(it=='a')
        flag =1;
    }
    if(flag && t.size()==1)
    cout<<1;
    else if (t.size()>1)
    cout<<-1;
    else
    cout<<(long long) pow(2,s.size());}
}
